# mGene wrapper script to start the interpreter with the correct list of arguments

set -e

PROG=`basename $0`
DIR=`dirname $0`

echo ${PROG}: This program is part of mGene version 0.1 beta.
echo Please make sure you read the DISCLAIMER.
echo

exec ${DIR}/start_interpreter.sh ${PROG} "`${DIR}/genarglist.sh $@`"
